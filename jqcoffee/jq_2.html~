    <script type="text/javascript">
       $("nav li a").click(function(){
        var obj=$(this);
        var _target=$(obj).attr('href');

        $(".main").hide();
        $(_target).fadeIn('slow');

      });
 $(".list").click(function(){
        var obj1=$(this);
        var _menu=$(obj1).attr('href');

        $(_menu).toggle();


    });
 $(".m2").click(function(){
        var obj2=$(this);
        var _menu1=$(obj2).attr('href');
       $(".dd").hide();

        $(_menu1).fadeIn("slow");


    });
 $(".m1").click(function(){
        var obj2=$(this);
        var _menu1=$(obj2).attr('href');
       $(".dd").hide();
        $(_menu1).fadeIn("slow");


    });




    </script>
